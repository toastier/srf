{
  "env" : {
    "node" : false,
    "browser": true
  },
  "rules": {
    "no-shadow": 0,
    "no-use-before-define": [2, "nofunc"],
    "strict": [2, "function"],
    "quotes": [1, "single"],
    "camelcase": [2, {"properties": "never"}],
    "no-trailing-spaces": [2, { "skipBlankLines": true }]
  },
  "plugin": "angular",
  "globals": {
    "angular": true
  }
}