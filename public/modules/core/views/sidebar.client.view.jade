.col-sm-3.col-md-2.sidebar#main_menu-menu(ng-controller="SidebarController as sidebar")
    ul.nav.nav-sidebar
        li(class="{{ item.itemClass }}", ng-repeat="item in sidebar.menu.items | orderBy : '+position'", ng-if="item.shouldRender(sidebar.user)")
            a(ng-href="/{{item.uiRoute}}") {{ item.title }}
