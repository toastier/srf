      du-grid-file-metadata-view(
      heading="Cover Letter",
      file-metadata="vm.application.coverLetterFileMeta"
      )
    .col-xs-12
      du-grid-file-metadata-view(
      heading="Curriculum Vitae",
      file-metadata="vm.application.cvFileMeta"
      )
