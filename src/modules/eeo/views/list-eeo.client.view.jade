navigation
section
    eeo-report-search-criteria
    eeo-report-applicants-hired(data-ng-show="vm.hired.length > 0")
    .row
        .col-xs-12.col-md-6
            .panel.panel-info
                .panel-heading Applicants by Gender
                .panel-body
                  table.table-bordered.report-tbl.col-xs-12.col-md-6
                    thead
                      tr
                        th.col-xs-8 Gender
                        th.col-xs-4
                          span.pull-right Total
                    tbody
                      tr(data-ng-repeat="item in vm.eeoData.byGender | toArray | orderBy : 'orderBy'")
                        td.col-xs-8 {{ item.label }}
                        td.col-xs-4
                          span.pull-right {{ item.count.self }}
                      tr
                        td.col-xs-8 Unknown
                        td.col-xs-4
                            span.pull-right {{ vm.applicationCount - vm.eeoData.totalCounts.byGender }}
                      tfoot
                        td.col-xs-8 Total
                        td.col-xs-4
                          span.pull-right {{ vm.applicationCount }}
        .col-xs-12.col-md-6
            .panel.panel-info
                .panel-heading Interviewed by Gender
                .panel-body
                  table.table-bordered.report-tbl.col-xs-12.col-md-6
                    thead
                      tr
                        th.col-xs-8 Gender
                        th.col-xs-4
                          span.pull-right Total
                    tbody
                      tr(data-ng-repeat="item in vm.eeoData.byGender | toArray | orderBy : " +
                      "'orderBy'")
                        td.col-xs-8 {{ item.label }}
                        td.col-xs-4
                          span.pull-right {{ item.count.interview }}
                      tr
                        td.col-xs-8 Unknown
                        td.col-xs-4
                            span.pull-right {{ vm.interviewCount - vm.eeoData.totalCounts.byGender }}
                      tfoot
                        td.col-xs-8 Total
                        td.col-xs-4
                          span.pull-right {{ vm.interviewCount }}
