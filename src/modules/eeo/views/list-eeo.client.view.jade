navigation
section
    eeo-report-search-criteria
    eeo-report-applicants-hired(data-ng-show="vm.hired.length > 0")
    .panel.panel-info
        .panel-heading Applicants by Gender
        .panel-body
          table.table-bordered.table-striped.col-xs-12.col-md-6
            thead
              tr
                th.col-xs-8 Gender
                th.col-xs-4
                  span.pull-right Count
            tbody
                  tr(data-ng-repeat="item in vm.eeoData.byGender.self | toArray | orderBy : 'orderBy'")
                td.col-xs-8 {{ item.label }}
                td.col-xs-4
                  span.pull-right {{ item.count }}
              tr
                td.col-xs-8 Unknown
                td.col-xs-4
                    span.pull-right {{ vm.applicationCount - vm.eeoData.totalCounts.byGender }}
              tr
                td.col-xs-8 TOTAL
                td.col-xs-4
                  span.pull-right {{ vm.applicationCount }}
    .panel.panel-info
        .panel-heading Interviewed by Gender
        .panel-body
          table.table-bordered.table-striped.col-xs-12.col-md-6
            thead
              tr
                th.col-xs-8 Gender
                th.col-xs-4
                  span.pull-right Count
            tbody
              tr(data-ng-repeat="item in vm.eeoData.byGender.interview | toArray | orderBy : " +
              "'orderBy'")
