    .panel.panel-info
        .panel-heading Applicants Identifying as Veterans
        .panel-body
          table.table-bordered.table-striped.col-xs-12
              thead
                  tr
                      th.col-xs-3 Veteran Identification
                      th.col-xs-2(data-ng-repeat="item in vm.eeoData.byGender | toArray | " +
                      "orderBy : 'orderBy'")
                          span.pull-right {{ item.label }}
                      th.col-xs-1
                        span.pull-right Unknown
                      th.col-xs-2
                          span.pull-right Total
              tbody
                  tr(ng-repeat-start="item in vm.eeoData.byVeteran | toArray | orderBy : " +
                  "'orderBy'")
                      td.col-xs-3 {{ item.label }}
                      td.col-xs-2(data-ng-repeat="gender in vm.eeoData.byGender | toArray | " +
                      "orderBy : " +
                      "'orderBy'")
                          span.pull-right {{ item.counts[gender.code] }}
                      td.col-xs-1
                          span.pull-right -
                      td.col-xs-2
                          span.pull-right {{ item.counts.totalCount }}
                  tr(ng-repeat-start="class in item.classes | toArray | orderBy : " +
                  "'orderBy'")
                      td.col-xs-3
                          span(style="padding-left:20px") {{ class.label }}
                      td.col-xs-2(data-ng-repeat="gender in vm.eeoData.byGender | toArray | " +
                      "orderBy : " +
                      "'orderBy'")
                        span.pull-right {{ class.counts[gender.code] }}
                      td.col-xs-1
                        span.pull-right -
                      td.col-xs-2
                          span.pull-right {{ class.counts.totalCount }}
                  tr(ng-repeat-end, ng-hide="true")
                  tr(ng-repeat-end, ng-hide="true")
                  tr
                    td.col-xs-3 Unknown
                    td.col-xs-2(data-ng-repeat="item in vm.eeoData.byGender | toArray | " +
                    "orderBy : " +
                    "'orderBy'")
                        span.pull-right -
                    td.col-xs-1
                        span.pull-right {{ vm.applicationCount - vm.eeoData.totalCounts.byVeteran }}
                    td.col-xs-2
                      span.pull-right {{ vm.applicationCount - vm.eeoData.totalCounts.byVeteran }}

                  tr
